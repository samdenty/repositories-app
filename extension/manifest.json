{
  "manifest_version": 2,
  "name": "Repositories",
  "version": "1.0",
  "description": "",
  "background": {
    "scripts": ["dist/background.js"],
    "persistent": true
  },
  "permissions": ["file:///*", "http://*/*", "https://*/*", "nativeMessaging"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["dist/content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["dist/*"]
}
